{
  "include_patterns": [
    "**/*.py",
    "**/*.cpp",
    "**/*.cc",
    "**/*.c",
    "**/*.hpp",
    "**/*.h",
    "**/ya.make",
    "**/CMakeLists.txt",
    "**/*.ya",
    "**/*.proto",
    "**/*.protobuf",
    "**/*.config",
    "**/*.conf",
    "**/*.cfg",
    "**/*.ini",
    "**/*.toml",
    "**/*.yaml",
    "**/*.yml",
    "**/*.json",
    "**/*.md",
    "**/*.markdown"
  ],
  "exclude_patterns": [
    "**/build/**",
    "**/__pycache__/**",
    "**/node_modules/**",
    "**/contrib/**",
    "**/*.pb.*",
    "**/third_party/**",
    "**/.git/**",
    "**/venv/**",
    "**/.venv/**",
    "**/veydb/**",
    "**/env/**",
    "**/.env/**",
    "**/virtualenv/**",
    "**/.virtualenv/**",
    "**/dist/**",
    "**/build/**",
    "**/.pytest_cache/**",
    "**/.mypy_cache/**",
    "**/.tox/**",
    "**/.coverage/**",
    "**/htmlcov/**",
    "**/.idea/**",
    "**/.vscode/**",
    "**/*.pyc",
    "**/*.pyo",
    "**/*.pyd",
    "**/__pycache__/**",
    "**/*.egg-info/**",
    "**/.eggs/**"
  ],
  "exclude_directories": [
    "build",
    "__pycache__",
    "node_modules",
    "contrib",
    "third_party",
    ".git",
    "venv",
    ".venv",
    "veydb",
    "env",
    ".env",
    "virtualenv",
    ".virtualenv",
    "dist",
    ".pytest_cache",
    ".mypy_cache",
    ".tox",
    ".coverage",
    "htmlcov",
    ".idea",
    ".vscode",
    ".eggs"
  ],
  "exclude_file_extensions": [
    ".pyc",
    ".pyo",
    ".pyd",
    ".pb.h",
    ".pb.cc",
    ".pb.cpp"
  ]
}
