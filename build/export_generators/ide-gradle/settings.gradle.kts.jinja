rootProject.name = "{{ project_name }}"

{%  if subdirs|length > 1 -%}
{%-     for subdir in subdirs -%}
{%-         set classname = subdir | replace("/", ":") %}
include(":{{ classname }}")
project(":{{ classname }}").projectDir = file("{{ arcadia_root }}/{{ subdir }}")
{%      endfor -%}
{%- endif -%}

{%- include "[generator]/debug.jinja" ignore missing -%}
