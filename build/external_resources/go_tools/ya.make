    VERSION(1.23)
    DECLARE_EXTERNAL_HOST_RESOURCES_BUNDLE_BY_JSON(GO_TOOLS go1.23.json)
ELSEIF(GOSTD_VERSION == 1.24)
    VERSION(1.24)
    DECLARE_EXTERNAL_HOST_RESOURCES_BUNDLE_BY_JSON(GO_TOOLS go1.24.json)
ELSEIF(GOSTD_VERSION == 1.25)
