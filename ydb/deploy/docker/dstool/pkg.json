{
  "meta": {
    "name": "ydb-dstool",
    "maintainer": "ydb <ydb@yandex-team.ru>",
    "description": "Package with opensource ydb-dstool app",
    "version": "{branch}.{revision}"
  },
  "params": {
    "docker_build_network": "host",
    "docker_registry": "cr.yandex",
    "docker_repository": "crp2lrlsrs36odlvd8dv",
    "docker_target": "release"
  },
  "build": {
    "build_type": "release",
    "targets": [
        "ydb/apps/dstool"    
    ],
    "flags": [],
    "target-platforms": [
      "default-linux-x86_64"
    ]
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "ydb/deploy/docker/dstool/Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "AUTHORS"
      },
      "destination": {
        "path": "/AUTHORS"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "LICENSE"
      },
      "destination": {
        "path": "/LICENSE"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "ydb/apps/dstool/README.md"
      },
      "destination": {
        "path": "/README.md"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "ydb/apps/dstool/version.txt"
      },
      "destination": {
        "path": "/version.txt"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "ydb/apps/dstool/ydb-dstool"
      },
      "destination": {
        "path": "/ydb-dstool",
        "attributes": {
          "mode": {
            "value": "a+x"
          }
        }
      }
    }
  ]
}
