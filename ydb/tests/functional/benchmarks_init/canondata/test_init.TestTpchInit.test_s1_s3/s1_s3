    AUTH_METHOD="NONE"
);

CREATE EXTERNAL TABLE `/Root/db/Root/db/tpch/s1/customer` (
    c_acctbal Double NOT NULL,
    c_address Utf8 NOT NULL,
    c_comment Utf8 NOT NULL,
    c_custkey Int64 NOT NULL,
    c_mktsegment Utf8 NOT NULL,
    c_name Utf8 NOT NULL,
    c_nationkey Int32 NOT NULL,
    c_phone Utf8 NOT NULL
)
WITH (
    DATA_SOURCE = "/Root/db/Root/db/tpch/s1_s3_external_source", FORMAT = "parquet", LOCATION = "h/s1/parquet/customer/"
);

