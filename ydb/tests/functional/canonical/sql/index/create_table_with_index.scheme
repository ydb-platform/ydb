--!syntax_v1
CREATE TABLE NewTableWithIndex (
    Key1 Int32,
    Index1A Int32,
    Index1B String,
    Index2 String,
    Value String,
    PRIMARY KEY (Key1),
    INDEX NewIndex1 GLOBAL ON (Index1A, Index1B),
    INDEX NewIndex2 GLOBAL ON (Index2)
);

CREATE TABLE personal_storefront
(
    hashOrderDate Uint64,
    orderDate String,
    orderId Uint64,
    buyerFullNamePdId String,
    recipientFullNamePdId String,
    courierAddressPdId String,
    PRIMARY KEY (hashOrderDate, orderDate, orderId)
);
