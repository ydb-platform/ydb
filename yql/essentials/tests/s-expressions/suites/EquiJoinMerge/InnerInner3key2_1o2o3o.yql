(let join (EquiJoin '(table1 'a) '(table2 'b) '(table3 'c) 
  '('Inner
    '('Inner '"a" '"b" '('"a" '"key" '"a" '"subkey") '('"b" '"key" '"b" '"subkey") '()) 
