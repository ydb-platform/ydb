(
(let config (DataSource 'config))
(let world (Configure! world config 'PureDataSource 'yt))

(let list (AsList (String 'a) (String 'b) (String 'c)))
(let data (Condense1 list
   (lambda '(item)
       '(item))
   (lambda '(item state) (Bool 'false))
   (lambda '(item state) (block '(
