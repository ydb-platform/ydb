/* hybridfile can not - missing langver support */
PRAGMA YqlSelect = 'force';

SELECT
    c_count,
    Count(*) AS custdist
FROM (
    SELECT
        c_custkey,
        Count(o_orderkey) AS c_count
    FROM
        plato.customer
    LEFT OUTER JOIN
        plato.orders
    ON
        c_custkey == o_custkey
        AND o_comment NOT LIKE '%special%requests%'
    GROUP BY
        c_custkey
)
GROUP BY
    c_count
ORDER BY
    custdist DESC,
    c_count DESC
;
