/* dqfile can not */
/* hybridfile can not - missing langver support */
PRAGMA YqlSelect = 'force';
PRAGMA AnsiImplicitCrossJoin;

SELECT
    s_name,
    s_address
FROM
    plato.supplier
,
    plato.nation
WHERE
    s_suppkey IN (
        SELECT
            ps_suppkey
        FROM
            plato.partsupp
        WHERE
            ps_partkey IN (
                SELECT
                    p_partkey
                FROM
                    plato.part
                WHERE
                    p_name LIKE 'forest%'
            )
            AND ps_availqty > (
                SELECT
                    0.5 * Sum(l_quantity)
                FROM
                    plato.lineitem
                WHERE
                    l_partkey == ps_partkey
                    AND l_suppkey == ps_suppkey
                    AND l_shipdate >= Date('1994-01-01')
                    AND l_shipdate < DateTime::MakeDate(DateTime::ShiftYears(Date('1994-01-01'), 1))
            )
    )
    AND s_nationkey == n_nationkey
    AND n_name == 'CANADA'
ORDER BY
    s_name
;
